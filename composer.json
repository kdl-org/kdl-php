{
    "name": "shieldo/kdl-php",
    "description": "A PHP implementation for the KDL data format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "dgreenshields@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shieldo\\Kdl\\": "src"
        },
        "files": [
            "src/grammar.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shieldo\\Kdl\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "mathiasverraes/parsica": "^0.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.13",
        "vimeo/psalm": "^4.3.2",
        "squizlabs/php_codesniffer": "^3.5.8",
        "ext-json": "*"
    }
}
