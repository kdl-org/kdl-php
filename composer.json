{
    "name": "kdl/kdl",
    "description": "A PHP implementation for the KDL data format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "dgreenshields@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kdl\\Kdl\\": "src"
        },
        "files": [
            "src/grammar.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kdl\\Kdl\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "parsica-php/parsica": "^0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "vimeo/psalm": "^4.3.2",
        "squizlabs/php_codesniffer": "^3.5.8",
        "ext-json": "*",
        "phpbench/phpbench": "^1.0"
    }
}
