  "ci": [
    {
      "name": "name",
      "values": ["CI"]
    },
    {
      "name": "on",
      "values": ["push", "pull_request"]
    },
    {
      "name": "env",
      "children": [
        {
          "name": "RUSTFLAGS",
          "values": ["-Dwarnings"]
        }
      ]
    },
    {
      "name": "jobs",
      "children": [
        {
          "name": "fmt_and_docs",
          "values": ["Check fmt & build docs"],
          "children": [
            {
              "name": "runs-on",
              "values": ["ubuntu-latest"]
            },
            {
              "name": "steps",
              "children": [
                {
                  "name": "step",
                  "properties": { "uses": "actions/checkout@v1" }
                },
                {
                  "name": "step",
                  "values": ["Install Rust"],
                  "properties": { "uses": "actions-rs/toolchain@v1" },
                  "children": [
                    {
                      "name": "profile",
                      "values": ["minimal"]
                    },
                    {
                      "name": "toolchain",
                      "values": ["stable"]
                    },
                    {
                      "name": "components",
                      "values": ["rustfmt"]
                    },
                    {
                      "name": "override",
                      "values": [true]
                    }
                  ]
                },
                {
                  "name": "step",
                  "values": ["rustfmt"],
                  "properties": { "run": "cargo fmt --all -- --check" }
                },
                {
                  "name": "step",
                  "values": ["docs"],
                  "properties": { "run": "cargo doc --no-deps" }
                }
              ]
            }
          ]
        },
        {
          "name": "build_and_test",
          "values": ["Build & Test"],
          "children": [
            {
              "name": "runs-on",
              "values": ["${{ matrix.os }}"]
            },
            {
              "name": "strategy",
              "children": [
                {
                  "name": "matrix",
                  "children": [
                    {
                      "name": "rust",
                      "values": ["1.46.0", "stable"]
                    },
                    {
                      "name": "os",
                      "values": ["ubuntu-latest", "macOS-latest", "windows-latest"]
                    }
                  ]
                }
              ]
            },
            {
              "name": "steps",
              "children": [
                {
                  "name": "step",
                  "properties": { "uses": "actions/checkout@v1" }
                },
                {
                  "name": "step",
                  "values": ["Install Rust"],
                  "properties": { "uses": "actions-rs/toolchain@v1" },
                  "children": [
                    {
                      "name": "profile",
                      "values": ["minimal"]
                    },
                    {
                      "name": "toolchain",
                      "values": ["${{ matrix.rust }}"]
                    },
                    {
                      "name": "components",
                      "values": ["clippy"]
                    },
                    {
                      "name": "override",
                      "values": [true]
                    }
                  ]
                },
                {
                  "name": "step",
                  "values": ["Clippy"],
                  "properties": { "run": "cargo clippy --all -- -D warnings" }
                },
                {
                  "name": "step",
                  "values": ["Run tests"],
                  "properties": { "run": "cargo test --all --verbose" }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
